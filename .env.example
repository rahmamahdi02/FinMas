# Finance Agent System Environment Variables
# Copy this file to .env and fill in your actual API keys

# =============================================================================
# FINANCIAL DATA APIs
# =============================================================================

# Finnhub API - Get your free key at https://finnhub.io/dashboard
FINNHUB_API_KEY=your_finnhub_api_key_here

# OpenBB Platform Token - Get from https://my.openbb.co/
OPENBB_PAT=your_openbb_token_here

# SEC API - Register at https://sec-api.io/
SEC_API_KEY=your_sec_api_key_here

# =============================================================================
# SOCIAL MEDIA APIs
# =============================================================================

# Reddit API - Create app at https://www.reddit.com/prefs/apps
REDDIT_CLIENT_ID=your_reddit_client_id_here
REDDIT_CLIENT_SECRET=your_reddit_client_secret_here

# =============================================================================
# AI/ML APIs
# =============================================================================

# OpenAI API - Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# LangSmith (Optional) - For LangChain tracing
LANGSMITH_API_KEY=your_langsmith_api_key_here

# =============================================================================
# DATABASE & STORAGE (Optional)
# =============================================================================

# Qdrant Vector Database - For semantic search
QDRANT_CLUSTER_KEY=your_qdrant_cluster_key_here

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================

# Environment (development, staging, production)
ENVIRONMENT=development

# Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
LOG_LEVEL=INFO
